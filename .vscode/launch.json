{
	"version": "0.2.0",
	"configurations": [
	  {
		"name": "Debug C Program",
		"type": "cppdbg",
		"request": "launch",
		"program": "${fileDirname}/so_long",
		"args": [
			"maps/map2.ber"
		],
		"stopAtEntry": false,
		"cwd": "${fileDirname}",
		"environment": [],
		"externalConsole": false,
		"MIMode": "gdb",
		"setupCommands": [
		  {
			"description": "Enable pretty-printing for gdb",
			"text": "-enable-pretty-printing",
			"ignoreFailures": true
		  }
		]
	  }
	]
  }